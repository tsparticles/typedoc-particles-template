{
  "name": "typedoc-particles-theme",
  "version": "1.0.3",
  "description": "TypeDoc theme with tsParticles as animated background",
  "main": "index.js",
  "files": [
    "bin",
    "LICENSE"
  ],
  "keywords": [
    "typescript",
    "documentation",
    "generator",
    "typedocplugin",
    "typedoc",
    "tsparticles",
    "particles.js",
    "particles-js",
    "particles"
  ],
  "repository": "https://github.com/tsparticles/typedoc-particles-theme.git",
  "author": "Matteo Bruni <ar3s@icloud.com>",
  "license": "MIT",
  "scripts": {
    "webpack": "webpack -p",
    "build:helpers": "tsc -p tsconfig.helpers.json",
    "build:plugin": "tsc -p tsconfig.plugin.json",
    "build": "npm run webpack && npm run build:helpers && npm run build:plugin",
    "prepublishOnly": "npm run build"
  },
  "engines": {
    "node": ">= 8"
  },
  "dependencies": {
    "lunr": "^2.3.9",
    "tsparticles": "^1.17.10",
    "typedoc": "~0.17.8"
  },
  "devDependencies": {
    "@types/lunr": "^2.3.3",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^6.0.3",
    "css-loader": "^4.2.2",
    "file-loader": "^6.0.0",
    "mini-css-extract-plugin": "^0.11.0",
    "resolve-url-loader": "^3.1.1",
    "sass": "^1.26.10",
    "sass-loader": "^10.0.1",
    "ts-loader": "^8.0.3",
    "typescript": "^4.0.2",
    "url-loader": "^4.1.0",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12",
    "webpack-merge": "^5.1.2"
  }
}
